type Query {
    totalClicks(dimensions: [QueryParams]!): Int!
    campaignMetrics: [DataMetrics]
}

type DataMetrics {
    dataSource: DataSource!
    campaign: Campaign!
    daily: String!
    clicks: Int
    impressions: Int
}

type DataSource {
    name: String
}

type Campaign {
    name: String
}

input QueryParams  {
    name: String!
    dimension: DimensionType!
}
enum DimensionType {
    DATA_SOURCE,
    CAMPAIGN
}

type Date {
    startDate: String,
    endDate: String
}
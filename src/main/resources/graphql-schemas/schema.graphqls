type Query {
    totalClicks(dimensions: Dimension): Int!
    campaignMetrics: [DataMetrics]
    campaigns: [Campaign]
}

enum Dimension {
    DATA_SOURCE
    CAMPAIGN
}

type DataMetrics {
    data_source: DataSource!
    campaign: Campaign!
    daily: String!
    clicks: Int
    impressions: Int
}

type DataSource {
    name: String
}

type Campaign {
    name: String
}
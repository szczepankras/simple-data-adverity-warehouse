type Query {
    filterByDates(queryParams: [QueryParams]!): [DataMetrics]
    filterByDateSource(queryParams: [QueryParams]!): [DataMetrics]
    filterByDatesAndDataSource(queryParams: [QueryParams]!): [DataMetrics]
    filterByCampaign(queryParams: [QueryParams]!): [DataMetrics]
    filterByDatesAndCampaign(queryParams: [QueryParams]!): [DataMetrics]
    campaignMetrics: [DataMetrics]
    totalClicksGroupedByDataSource(queryParams: [QueryParams]!): Float
    totalClicksGroupedByCampaign(queryParams: [QueryParams]!): Float
}

type DataMetrics {
    dataSource: String!
    campaign: String!
    date: String
    clicks: Int
    impressions: Int
    CTR: Float
}

input QueryParams  {
    name: String
    dateFrom: String
    dateTo: String
}
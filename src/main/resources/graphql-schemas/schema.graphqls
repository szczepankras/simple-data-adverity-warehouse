type Query {
    totalClicks(dimensions: [Dimension]): Int!
    campaignMetrics: [DataMetrics]
}

type DataMetrics {
    dataSource: DataSource!
    campaign: Campaign!
    daily: String!
    clicks: Int
    impressions: Int
}

type DataSource {
    name: String
}

type Campaign {
    name: String
}

input Dimension  {
    name: String!
}
type Query {
    totalClicks(queryParams: [QueryParams]!): Float
    campaignMetrics: [DataMetrics]
}

type DataMetrics {
    dataSource: DataSource!
    campaign: Campaign!
    date: String
    clicks: Int
    impressions: Int
}

type DataSource {
    name: String
}

type Campaign {
    name: String
}

input QueryParams  {
    name: String
    dateFrom: String
    dateTo: String
}